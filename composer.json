{
    "name": "andrevalentin/laravel-onfido",
    "description": "A Laravel wrapper for the Onfido PHP client",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "onfido", "identity"],
    "authors": [
        {
            "name": "Andr√© Valentin",
            "homepage": "https://github.com/andrevalentin"
        }
    ],
    "require": {
        "php": "^7.1.3 || ^8.0",
        "onfido/api-php-client": "^5.2 || ^6.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "graham-campbell/testbench": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "AndreValentin\\Onfido\\": "src/andrevalentin/Onfido"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndreValentin\\Onfido\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Onfido": "AndreValentin\\Onfido\\Facades\\Onfido"
            },
            "providers": [
                "AndreValentin\\Onfido\\OnfidoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
